;USRCLR.CP
;
RECORD	USRSEL
	.INCLUDE 'DEF:RD065A.DEF'
;
RECORD	VARS
	LOKCTL	,D1
	READ	,D1,0
	SWITCH	,D1
	V	,D1
;
PROC
	XCALL TERID(V)
	XCALL OUTPT (1,2,2,'CLEAR BILLING SELECT FILE',1)

	SWITCH = 5
	XCALL FILES (1,'SU',065,SWITCH)
LOOP,
	LOKCTL = 1
	XCALL IOS (1, USRSEL, READ, LOKCTL)
	IF (LOKCTL .NE. 0) GOTO EOF

	DELETE (1)
	GOTO LOOP
EOF,
	CLOSE 1
	XCALL PGCHN ('CP:UPDMIS',1)
	END

