CAL_CMC,	; Calc cornermatic corners...
	IF(CAT.EQ.3 .OR. CAT.EQ.5)RETURN	;4-PC, OVERSIZE => NO CMC
	CMQTY=
	SAVVAL = 'CM'
	CMQTY=ORG_CORN/2
	TCORN=ORG_CORN-CMQTY
	USING DUTYPE SELECT
	(1),	CMITM = 'CMGD'	;GALV
	(2),	CMITM = 'CMAD'	;ALUM
	(3),	CMITM = 'CMSD'	;SS
	(4),	CMITM = 'CMGD'	;Paintgrip
;;;	(5),	CMITM = 'DMGD'	;G-90	;changed to CMGD 5/15/02 per SteveM
	(5),	CMITM = 'CMGD'	;G-90
	(6),	CMITM = 'CMGD'	;PVS
	(7),	CMITM = 'CMGD'	;Other
	ENDUSING

	KEY = CMITM
	CALL ITMMAS
	IF (SRCCTL)
	THEN	BADCMC=1
	ELSE	BEGIN
		CMPRC = PRICE
		CMDES = DESCR
		END

	RETURN
