	CALL CHKITM

	XCALL W_DISP(W_ID,WD_POS,ROW,31,TR1ITM)
	XCALL W_UPDT
	KEY = TR1ITM
	CALL ITMMAS
	IF (SRCCTL)
		BEGIN
		BADTR1=1
		RETURN
		END

	BADTR1=0
	TR1PRC = PRICE
	TR1DES = DESCR
	TR1DES(1,3) = SZ1, 'ZZX' [LEFT]
	D3 = %TRIM(TR1DES(1,3))
	INCR D3
	TR1DES(D3,D3) = '/'
	INCR D3
	TR1DES(D3,D3+1) = SZ2, 'ZX' [LEFT]

