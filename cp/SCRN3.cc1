	TGADES	=		;---------------------
	CTL = '17,19,05,00,# '
	XCALL W_DISP(W_ID,WD_POS,ROW,COL,WD_CLR,WDC_EOL)

;;;	TGAQTY = X_WGHT / ((DUCLBS * SIZE3) #3)
	FACTOR=(DUCLBS*SIZE3)#3
