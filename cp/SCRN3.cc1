CAL_RANGLE,
	IF (.NOT. DRANGL) RETURN
	IF (ACC .NE. 1) RETURN

	SAVVAL = DPUNCH
	TR1QTY	=		;---------------------
	TR1ITM	= 		;Tie rods 1
	TR1PRC	=		;
	TR1DES	=		;---------------------
	
